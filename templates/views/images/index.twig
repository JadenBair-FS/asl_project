{% extends "layouts/base.twig" %} {% block main_content %}
<h1>All Products</h1>
{% for image in images %}
<li>
  ID: {{ image.id }}
  <br />
  <button><a href="/images/{{ image.id }}">Edit</a></button>
  <form method="POST" action="/images/{{ image.id }}/delete">
    <input type="submit" value="Delete" />
  </form>
</li>
{% endfor %}
<br />
<button><a href="/images/new">Add New Image</a></button>
{% endblock %}
